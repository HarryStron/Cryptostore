            <Button fx:id="deleteBtn" contentDisplay="CENTER" mnemonicParsing="false" text="Delete" HBox.hgrow="NEVER" HBox.margin="$x1" />
          </children>
        </HBox>
      </top>
    </BorderPane>
  </children>
</AnchorPane>
