      <bottom>
        <HBox prefHeight="-1.0" prefWidth="600.0" spacing="10.0" BorderPane.alignment="CENTER_LEFT">
          <children>
            <Button fx:id="createUserBtn" mnemonicParsing="false" onAction="#createUser" text="CreateUser" />
