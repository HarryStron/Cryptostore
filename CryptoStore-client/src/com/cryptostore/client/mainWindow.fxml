<AnchorPane id="AnchorPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="400.0" minWidth="600.0" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2">
  <children>
    <BorderPane prefHeight="400.0" prefWidth="600.0" visible="true" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <bottom>
        <HBox prefHeight="-1.0" prefWidth="600.0" spacing="10.0" BorderPane.alignment="CENTER_LEFT">
          <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="User: Admin1">
              <HBox.margin>
                <Insets fx:id="x1" />
              </HBox.margin>
            </Text>
            <Separator prefHeight="-1.0" prefWidth="-1.0" HBox.hgrow="ALWAYS" />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Status: Connected" HBox.margin="$x1" />
            <Separator prefHeight="-1.0" prefWidth="-1.0" HBox.hgrow="ALWAYS" />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Space used: 10MB" HBox.margin="$x1" />
          </children>
          <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" fx:id="x2" />
          </padding>
        </HBox>
      </bottom>
      <center>
        <ListView fx:id="listView" prefHeight="200.0" prefWidth="200.0" />
      </center>
      <top>
        <HBox padding="$x2" prefHeight="-1.0" prefWidth="600.0" spacing="10.0" BorderPane.alignment="CENTER_RIGHT">
          <children>
            <Button fx:id="backBtn" mnemonicParsing="false" onAction="#handleBackbuttonClick" text="Back" />
